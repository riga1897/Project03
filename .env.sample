# Конфигурация базы данных PostgreSQL
# Вариант 1: Полный URL подключения (рекомендуется)
# DATABASE_URL=postgresql://username:password@hostname:port/database_name

# Вариант 2: Отдельные параметры подключения
# PGHOST=localhost
# PGPORT=5432
# PGDATABASE=job_search_app
# PGUSER=postgres
# PGPASSWORD=your_password

# API ключи
# SuperJob API ключ (получить на https://api.superjob.ru/register/)
# SUPERJOB_API_KEY=your_superjob_api_key_here

# Настройки фильтрации вакансий
# Загружать только вакансии с указанной зарплатой (true/false)
FILTER_ONLY_WITH_SALARY=false

# Настройки кэширования
CACHE_TTL=3600

# Настройки логирования
LOG_LEVEL=INFO

# Таймауты подключения к базе данных
PGCONNECT_TIMEOUT=10
PGCOMMAND_TIMEOUT=30